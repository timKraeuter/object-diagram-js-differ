    "all": "run-s lint test",
    "lint": "eslint .",
    "lintFix": "eslint . --fix",
    "test": "jasmine",
    "build": "rollup -c --bundleConfigAsCjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timKraeuter/object-diagram-js-differ"
  },
  "main": "dist/index.cjs",
