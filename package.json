    "jasmine": "^5.1.0",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-syntax-import-assertions": "^7.24.1",
    "rollup": "^4.13.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4"
